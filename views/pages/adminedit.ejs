<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Document</title>
    <%include ../partials/css %>
</head>

<body>
    <div class="container">
        <%include ../partials/nav %>
            <form id="editForm">
                <label for="name">navn</label>
                <input type="text" id="name" name="name" value="<%=data[0].name%>">
                <label for="time">tid</label>
                <input type="date" id="time" name="time" value="<%=data[0].time%>">
                <label for="duration">varighed</label>
                <input type="number" id="duration" name="duration" value="<%=data[0].duration%>">
                <select id="location">
                    <option value="0">vælg sted</option>
                    <%room.forEach((element)=>{%>
                        <option value="<%=element.id%>">
                            <%=element.name%>
                        </option>
                        <%})%>
                </select>
                <label for="price">pris</label>
                <input type="number" id="price" name="price" value="<%=data[0].price%>">
                <select id="category">
                    <option value="0">vælg kategori</option>
                    <%cat.forEach((element)=>{%>
                        <option value="<%=element.id%>">
                            <%=element.name%>
                        </option>
                        <%})%>
                </select>
                <button>ændre</button>
            </form>
    </div>
    <%include ../partials/js %>
        <script src="/js/adminedit.js"></script>
</body>

</html>